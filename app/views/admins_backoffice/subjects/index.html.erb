<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Listing All Subjects</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
      <thead>
          <tr>
              <th>#</th>
              <th><%#= translate_attribute(@subjects, :description) %></th>
              <th>
                <%= link_to new_admins_backoffice_subject_path, class:"btn btn-success btn-circle" do %>
                  <i class="fa fa-plus"></i>
                <% end %>
                <%= link_to admins_backoffice_subjects_path(format: :pdf), class:"btn btn-danger btn-circle" do %>
                  <i class="fa fa-file-pdf-o"></i>
                <% end %>
              </th>
          </tr>
      </thead>
      <p><%= link_to 'New Subject', new_admins_backoffice_subject_path %></p>
      <tbody>
          <% @subjects.each do |subject| %>
          <tr>
              <td><%= subject.id %></td>
              <td><%= subject.description %></td>
              <td style="width: 100px">
                <%= link_to "Edit", edit_admins_backoffice_subject_path(subject.id) %>
                <%#= link_to edit_admins_backoffice_subject_path(subject), class:"btn btn-primary btn-circle" do %>
                  <i class="fa fa-edit"></i>
                <%# end %>
                
                <%= button_to "Delete", admins_backoffice_subject_path(subject.id), method: :delete, data: { confirm: 'Are you sure ?' } %>
                
              </td>
          </tr>
          <% end %>
      </tbody>
  </table>

  <div class="text-center">
    <%= paginate @subjects %>
  </div>
</div>
