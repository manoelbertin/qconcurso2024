<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Listing Administrators</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

  <p>
      <%= link_to "Come Back Site", site_welcome_index_path %>
  </p>
  <p>
   <%#= @admins.inspect %>
  </p>
<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
      <thead>
          <tr>
              <th>#</th>
              <th> emails bellow:</th>
              <th>
                <%#= link_to new_admins_backoffice_admin_path, class:"btn btn-success btn-circle" do %>
                  <!--<i class="fa fa-plus"></i>-->
                <%# end %>
              </th>
          </tr>
      </thead>
      <p><%= link_to 'New Administrator', new_admins_backoffice_admin_path %></p>
      <tbody>
          <% @admins.each do |admin| %>
          <tr>
              <td><%= admin.id %></td>
              <td><%= admin.email %></td>
              <td style="width: 100px">
                <%= link_to "Edit", edit_admins_backoffice_admin_path(admin.id) %>
                <%#= link_to edit_admins_backoffice_admin_path(admin.id), class:"btn btn-primary btn-circle" do %>
                  <i class="fa fa-edit"></i>
                <%# end %>

                <%= button_to "Delete", admins_backoffice_admin_path(admin.id), method: :delete, data: { confirm: 'Are you sure ?' } %>
                
              </td>
          </tr>
          <% end %>
      </tbody>
  </table>
    <div class="text-center">
      <%= paginate @admins %>
    </div>
</div>
